<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <DriverConfig>
        <LocalBrowser>chrome</LocalBrowser>
        <Url>https://demoqa.com/</Url>
        <Headless>false</Headless>
        <Gpu>--disable-gpu</Gpu>
        <Infobars>--disable-infobars</Infobars>
        <Extensions>--disable-extensions</Extensions>
        <Resolution>window-size=1920,1080</Resolution>
        <Sandbox>--no-sandbox</Sandbox>
        <BrowserCheck>--no-default-browser-check</BrowserCheck>
        <DevShm>--disable-dev-shm-usage</DevShm>
    </DriverConfig>
    <DatabaseConfig>
        <Port>3306</Port>
        <Database>itschooldemoqadb</Database>
        <Username>root</Username>
        <Password>root1234</Password>
    </DatabaseConfig>
</configuration>